extends base.pug

block scripts
  script(src='/static/index.js')

block style 
    style
      include ../static/style.css
      include ../static/styleContact.css

block content 
    div.container
        h3 Contact Us
        form(action="/contact", methods="post", class="myForm")
           input(type="text", class="myInput", name="name", placeholder="Enter your name")
           input(type="number", class="myInput", name="phone", placeholder="Enter your Phone Number")
           input(type="email", class="myInput", name="email", placeholder="Enter your Email Id")
           input(type="text", class="myInput", name="address", placeholder="Enter your Address")
           input(type="number", class="myInput", name="pincode", placeholder="Enter your PinCode")
           button(type="submit" class="btn" value="submit")
              | Submit
        